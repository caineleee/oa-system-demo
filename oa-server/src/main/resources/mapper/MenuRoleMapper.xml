<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lee.oa.mapper.MenuRoleMapper">
    <resultMap id="BaseResultMap" type="com.lee.oa.pojo.MenuRole">
        <id column="id" property="id"/>
        <result column="menu_id" property="menuId"/>
        <result column="role_id" property="roleId"/>
    </resultMap>
    <insert id="insertRecords">
        insert into t_menu_role (menu_id, role_id) values
        <foreach collection="menuIds" item="mid" separator=",">
            (#{mid}, #{roleId})
        </foreach>
    </insert>
</mapper>